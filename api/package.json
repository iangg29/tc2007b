{
  "name": "tc2007b-api",
  "version": "1.0.0",
  "description": "Application's API server",
  "main": "source/server.ts",
  "scripts": {
    "start": "cd build && node server.js",
    "dev": "nodemon source/server.ts",
    "build": "rm -rf build/ && prettier --write source/ && tsc",
    "lint": "eslint source/*",
    "test": "jest --passWithNoTests",
    "format": "eslint source/* --fix",
    "prepare": "cd .. && husky install ./api/.husky"
  },
  "repository": {
    "type": "git",
    "url": "tc2007"
  },
  "keywords": [
    "api",
    "graphql",
    "database",
    "apollo",
    "mobile",
    "app"
  ],
  "author": "Ian Garcia, Erick Garcia, Fermin Mendez, Jose Angel",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^9.0.2",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.23.0",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^6.5.2",
    "graphql": "^16.6.0",
    "helmet": "^6.0.0",
    "knex": "^2.2.0",
    "knex-paginate": "^3.0.1",
    "lint-staged": "^13.0.3",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  }
}
